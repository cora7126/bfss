<!----------Quick Link Section -----Start------->
<div class="quick-link">
   <div class="row" style="margin-left: 0px;margin-right: 0px;">
      <div class="col-lg-9 ">
         <h1>Quick links</h1>
         <div class="row">
            <div class="col-md-3 mob_bottom_margin">
               <div class="quick-link-sub-sec">
                  <i class="fal fa-user"></i>
                  <p><a class="datainfo" href="/appointments/scheduled">View / Edit <br>
                     Users</a>
                  </p>
               </div>
            </div>
            <div class="col-md-3 mob_bottom_margin">
               <div class="quick-link-sub-sec">
                  <i class="far fa-clock"></i>
                  <p>Pending <br>
                     Assessments
                  </p>
               </div>
            </div>
            <div class="col-md-3 mob_bottom_margin">
               <a href='/bfssAthleteProfile' class="atheltic_link">
                  <div class="quick-link-sub-sec">
                     <i class="far fa-search"></i>
                     <p>Search <br />Athletes</p>
                  </div>
               </a>
            </div>
            <div class="col-md-3">
               <div class="quick-link-sub-sec">
                  <i class="fas fa-map-marker-plus"></i>
                  <p><a class="datainfo" href="/add_organization">Add New<br>
                     Organizations</a>
                  </p>
               </div>
            </div>
         </div>
         <div class="row  mt-5">
            <div class="col-md-3 mob_bottom_margin">
               <div class="quick-link-sub-sec">
                  <i class="fal fa-calendar-alt"></i>
                  <p><a class="datainfo" href="/appointments/scheduled">View / Edit <br>
                     Organizations</a>
                  </p>
               </div>
            </div>
            <div class="col-md-3 mob_bottom_margin">
               <div class="quick-link-sub-sec">
                  <i class="far fa-chart-bar"></i>
                  <p>Private <br>
                     Assessments
                  </p>
               </div>
            </div>
            <div class="col-md-3 mob_bottom_margin">
               <a href='/bfssAthleteProfile' class="atheltic_link">
                  <div class="quick-link-sub-sec">
                     <i class="far fa-users"></i>
                     <p>Group <br />Assessments</p>
                  </div>
               </a>
            </div>
            <div class="col-md-3">
               <div class="quick-link-sub-sec">
                  <i class="fas fa-ticket"></i>
                  <p><a class="datainfo" href="/faqs">Ticketing<br>
                     Systems</a>
                  </p>
               </div>
            </div>
         </div>
      </div>
      <div class="col-lg-3">
         {% if rolename == 'bfss_manager' %}
         <div class="my-assessment">
            <button class="accordion"><i class="fa fa-minus" aria-hidden="true"></i>Pending Approval</button>
            <div class="panel">
               <div class="panel_row">
                  <h3>Central Christan High School Whittler, CA</h3>
               </div>
               <div class="panel_row">
                  <h3>Bogle Jr. High Long Beach, CA</h3>
               </div>
               <div class="panel_row">
                  <h3>Washington High School La Habra, CA</h3>
               </div>
               <div class="panel_row">
                  <h3>EI Monte Community College EI Monte, CA</h3>
               </div>
            </div>
         </div>
         {% else %}
         <div class="my-assessment">
            <button class="accordion"><i class="fa fa-minus" aria-hidden="true"></i> MY ASSESSMENTS</button>
            <div class="panel">
               <div class="panel_row">
                  <h3>Elite Assessment</h3>
                  <p>December 29,2019</p>
               </div>
               <div class="panel_row">
                  <h3>Starter Assessment</h3>
                  <p>September 29,2019</p>
               </div>
               <div class="panel_row">
                  <h3>Starter Assessment</h3>
                  <p>March 29,2019</p>
               </div>
            </div>
         </div>
         {% endif %}
      </div>
   </div>
</div>
