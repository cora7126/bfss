<div class="calender_event_pop_up">
<div class="container">
	<div class="tophead">
		<div class="image">
			<img src='{{ data.field_image }}' alt='{{ data.title }}' />
		</div>
		<div class="title-timing">
			<div>
				{% if data.latest_timing %}
					<span class="timing-date">
					{{ data.latest_timing|date("M d") }}
					</span>
				{% endif %}
			</div>
			<div>
				<h2>
					{{ data.title }}
				</h2>
				<span class='loca'>{{ data.field_location|slice(0,27) }} </span>
			</div>
		</div>
	</div>
	<div class="ticketing">
		<div class="share">
		<span><i class="fas fa-share-alt"></i>  Share</span>
		</div>
		<div class="ticket">
		<span><i class="fa fa-ticket" aria-hidden="true"></i>   TICKETING</span>
		</div>
	</div>
	<div class="details">
		<div class="body">
			<div class='node-body'>
				{{ data.body }}
			</div>
		</div>
		<div class="date-location">
			<div class="date-and-time">
				<p>DATE AND TIME</p>
				{% if data.latest_timing %}
					<span class="timing-date">
						{{ data.latest_timing|date("F,") }}<br/>
						{{ data.latest_timing|date("M d, Y h:i A") }}
					</span>
				{% endif %}
				<p>UNTIL</p>
				<span>
				{{ data.latest_duration }}
				</span>

			<div>
			<div class="location">
				<p>LOCATION</p>
				{{ data.field_location }}
			<div>
			<div class="ticketing">
				<p>TICKETING</p>
				<a href="{{ data.url }}" class="book-now">Book Now</a>
			<div>
		</div>
	</div>
</div>
</div>